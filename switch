#include <stdio.h>
int main()
{
    int n;
    printf("enter number n ");
    scanf("%d",&n);
    switch(n)
    {
        case 21: printf("number is 21");
        break;
        case 22: printf("number is 22 ");
        break;
        default : printf("number is invalid");
        
    }
    
    
    
}
