// Online C compiler to run C program online
#include <stdio.h>
void swap(int *x, int *y)
{
    int temp;
    temp=*x;
    *x=*y;
    *y=temp;
}
main()
{
    int a,b;
    printf("enter your no a=");
    scanf("%d",&a);
    printf("enter your no b=");
    scanf("%d",&b);
    printf("\n before swapping ");
    printf("a=%d, b=%d",a,b);
    swap(&a,&b);
    printf("\n after swapping");
    printf("a=%d, b=%d",a,b);
}

    

