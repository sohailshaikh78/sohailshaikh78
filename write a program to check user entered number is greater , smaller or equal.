//write a program to check user entered number is greater, smaller or equal
#include <stdio.h>
main()
{ 
    int num;
    printf("enter number ");
    scanf("%d",&num);
     if(num>0)
    {
        printf("number is greater");
    }
    else if(num<0)
    {
        printf("number is smaller");
    }
    else
    {
        printf("number is equal");
    }
    

}
