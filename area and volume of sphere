//area and volume of sphere
#include <stdio.h>
main()
{
    const int pi=3.14;
    int r;
    int area;
    int volume;
    printf("enter radius of sphere in mm=");
    scanf("%d",&r);
    area= 4*pi*r*r 
    ;
    volume=(4/3)*pi*r*r*r;
    
    printf("area of sphere is %d sqmm",area);
    
    printf("\n");
    printf("volume of sphere is %d sqmm",volume);
    
    
}
