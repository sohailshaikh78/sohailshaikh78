//sumo of all no. using loop
#include <stdio.h>
int main()
{
    int i,n,fact=1;
   
    printf("enter a number n ");
    scanf("%d",&n);
  
     
    for(i=1;i<=n;i++)
    
    
    {
        fact=fact * i;
          
    }
    
        
      
      printf("fact is %d",fact);
     
        
     
    
    
        
    
}
