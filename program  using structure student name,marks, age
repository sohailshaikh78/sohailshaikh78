// program to display using structure student name,rollno,marks.
#include <stdio.h>
struct student
{
    char name[10];
    int marks;
    int age;
} s1 ;
  main()
{
     printf("enter a name=");
    scanf("%s",s1.name);
    printf("enter marks=");
    scanf("%d",&s1.marks);
    printf("enter age=");
    scanf("%d",&s1.age);
    

//display data
    printf("\n name is %s",s1.name);
    printf("\n marks is %d",s1.marks);
    printf("\n age is %d",s1.age);
}
